{
  "name": "ember-x-tabs",
  "version": "1.0.9",
  "description": "A mix of various tab component styles with theme customization options.",
  "keywords": [
    "ember-addon",
    "tabs",
    "component",
    "tab component",
    "tab"
  ],
  "license": "MIT",
  "author": "Rajasegar Chandran",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/rajasegar/ember-x-tabs",
  "scripts": {
    "build": "ember build --environment=production",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "ember test",
    "test:ember": "COVERAGE=true ember test",
    "test:all": "ember try:each",
    "test:firefox": "ember test --launch='Firefox'",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "broccoli-asset-rev": "^3.0.0",
    "coveralls": "^3.1.1",
    "ember-cli": "~3.16.2",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-page-object": "^1.15.3",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.16.10",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.18.1",
    "ember-try": "^1.2.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "loader.js": "^4.7.0",
    "prettier": "^2.5.1",
    "qunit-dom": "^0.8.5",
    "semantic-release": "^19.0.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://rajasegar.github.io/ember-x-tabs/"
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "package.json",
            "CHANGELOG.md"
          ],
          "message": "release(version): Release ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ],
      "@semantic-release/github"
    ]
  }
}
